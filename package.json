{
	"name": "rui",
	"displayName": "rui",
	"description": "rui 框架代码补充插件",
	"version": "1.0.0",
	"publisher": "rui",
	"icon": "images/logo.jpg",
	"engines": {
		"vscode": "^1.33.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tffe-team/rui-extensions.git"
  }, 
	"keywords": [
			"rui"
	],
	"categories": [
		"Snippets",
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.rui"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
      {
        "id": "ruiext",
        "aliases": ["ruiext"],
        "extensions": [".vue", ".html", ".ts"]
      }
    ],
		"commands": [
			{
				"command": "extension.rui",
				"title": "rui"
			}
		],
		"snippets": [
      {
        "language": "ruiext",
        "path": "./snippets/html.json"
      },
			{
        "language": "scss",
        "path": "./snippets/scss.json"
			}
    ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
